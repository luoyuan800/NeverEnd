<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.luo.yuan.maze"
          android:versionCode="5031"
          android:versionName="5.0.3.1">
    <uses-sdk android:minSdkVersion="19"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />

    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher"
            android:name=".client.service.NeverEnd"
                 android:theme="@android:style/Theme.Holo.Light"
            >
        <activity android:name=".client.display.activity.SelectedActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".client.display.activity.GameActivity"/>
        <activity android:name=".client.display.activity.OnlineActivity"/>
    </application>
</manifest>
